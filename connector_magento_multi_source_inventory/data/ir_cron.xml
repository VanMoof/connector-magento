<?xml version="1.0" encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="cron_export_stock_levels" model="ir.cron">
        <field name="name">Update Stock Levels in Magento</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_magento_stock_update"/>
        <field name="code">model.set_product_stock_levels_in_magento()</field>
    </record>
</odoo>
